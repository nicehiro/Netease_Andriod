<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.nicehiro.animation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".TweenActivity"></activity>
        <activity android:name=".InterpolatorActivity"></activity>
        <activity android:name=".FrameActivity"></activity>
        <activity android:name=".ObjectAnimationActivity"></activity>
        <activity android:name=".LayoutAnimatorActivity"></activity>
    </application>

</manifest>